
config policy-routing 'config'
	option loglevel '0'
	option enabled '0'

config interface
	option interface 'eth1'
	option strict '1'

config policy
	option interface 'eth1'
	option comment 'OpenDNS Hosts'
	option type 'remote_nets'
	option remote_nets '208.67.222.222 208.67.220.220 208.67.222.220 208.67.220.222 2620:0:ccc::2 2620:0:ccd::2'
